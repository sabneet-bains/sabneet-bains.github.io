---
layout: null
---
[
{% for page in site.pages %}
  {% if page.title %}
  {
    "title": "{{ page.title | escape }}",
    "url": "{{ page.url | relative_url }}",
    "excerpt": "{{ page.content | strip_html | strip_newlines | truncate: 160, '...' | escape }}"
  }{% unless forloop.last %},{% endunless %}
  {% endif %}
{% endfor %}
]
